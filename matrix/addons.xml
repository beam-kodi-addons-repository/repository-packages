<?xml version='1.0' encoding='UTF-8' standalone='yes'?>
<addons>
   <addon id='repository.beam.xbmc-addons' name='Beam Kodi Add-ons (Matrix)' version='2.1.0' provider-name='Beam'>
      <extension point='xbmc.addon.repository' name='Beam Kodi Add-ons Repository (Matrix)'>
         <info compressed='false'>https://xbmc-repo.bimovi.cz/matrix/addons.xml</info>
         <checksum>https://xbmc-repo.bimovi.cz/matrix/addons.xml.md5</checksum>
         <datadir zip='true'>https://xbmc-repo.bimovi.cz/matrix/packages/</datadir>
      </extension>
      <extension point='xbmc.addon.metadata'>
         <summary>Beam Kodi Add-ons Repository (Matrix)</summary>
         <description>Add-ons created by Andrew Beam</description>
         <platform>all</platform>
         <source>https://github.com/beam-kodi-addons-repository/repository.beam.xbmc-addons</source>
         <email>ondra@bimovi.cz</email>
      </extension>
   </addon>
   <addon id='script.tvpower.screensaver.control' name='TV power controller' version='2.1.1' provider-name='Beam'>
      <requires>
         <import addon='xbmc.python' version='3.0.0'/>
      </requires>
      <extension point='xbmc.python.script' library='program.py'/>
      <extension point='xbmc.service' library='service.py' start='login'/>
      <extension point='xbmc.addon.metadata'>
         <summary lang='en'>Turn off/on TV with screensaver</summary>
         <description lang='en'>Turn off/on TV with screensaver</description>
         <source>
            https://github.com/beam-kodi-addons-repository/script.tvpower.screensaver.control
         </source>
         <email>ondra@bimovi.cz</email>
      </extension>
   </addon>
   <addon id='service.subtitles.edna.cz' name='Edna.cz' version='2.0.2' provider-name='Beam'>
      <requires>
         <import addon='xbmc.python' version='3.0.0'/>
         <import addon='script.module.simplejson' version='3.17.0'/>
      </requires>
      <extension point='xbmc.subtitle.module' library='service.py'/>
      <extension point='xbmc.addon.metadata'>
         <summary>Edna.cz</summary>
         <description lang='en_GB'>Search and download subtitles from edna.cz (Czech/Slovak only)</description>
         <description lang='cs_CZ'>Vyhledavani a stahovani titulku z edna.cz</description>
         <description lang='sk_SK'>Vyhladavanie a stahovanie titulkov z edna.cz</description>
         <source>https://github.com/beam-kodi-addons-repository/service.subtitles.edna.cz</source>
         <email>ondra@bimovi.cz</email>
      </extension>
   </addon>
